{
  "logging": {
    "version": 1,
    "disable_existing_loggers": false,
    "root": {
      "handlers": [
        "file"
      ],
      "level": "DEBUG"
    },
    "loggers": {
      "idp": {
        "level": "DEBUG"
      }
    },
    "handlers": {
      "file": {
        "class": "logging.FileHandler",
        "filename": "debug.log",
        "formatter": "default"
      }
    },
    "formatters": {
      "default": {
        "format": "%(asctime)s %(name)s %(levelname)s %(message)s"
      }
    }
  },
  "port": "{PORT}",
  "domain": "{DOMAIN}",
  "base_url": "{BASE_URL}",
  "httpc_params": {
    "verify": false
  },
  "rp_keys": {
    "private_path": "private/jwks.json",
    "key_defs": [
      {
        "type": "RSA",
        "key": "",
        "use": [
          "sig"
        ]
      },
      {
        "type": "EC",
        "crv": "P-256",
        "use": [
          "sig"
        ]
      }
    ],
    "public_path": "static/jwks.json",
    "read_only": false
  },
  "services": {
    "webfinger": {
      "class": "oidcrp.oidc.webfinger.WebFinger",
      "kwargs": {}
    },
    "discovery": {
      "class": "oidcrp.oidc.provider_info_discovery.ProviderInfoDiscovery",
      "kwargs": {}
    },
    "registration": {
      "class": "oidcrp.oidc.registration.Registration",
      "kwargs": {}
    },
    "authorization": {
      "class": "oidcrp.oidc.authorization.Authorization",
      "kwargs": {}
    },
    "accesstoken": {
      "class": "oidcrp.oidc.access_token.AccessToken",
      "kwargs": {}
    },
    "userinfo": {
      "class": "oidcrp.oidc.userinfo.UserInfo",
      "kwargs": {}
    },
    "end_session": {
      "class": "oidcrp.oidc.end_session.EndSession",
      "kwargs": {}
    }
  },
  "clients": {
    "": {
      "client_preferences": {
        "application_name": "rphandler",
        "application_type": "web",
        "contacts": [
          "ops@example.com"
        ],
        "response_types": [
          "code",
          "code id_token",
          "code id_token token",
          "id_token token",
          "id_token",
          "code token"
        ],
        "scope": [
          "openid",
          "profile",
          "email",
          "address",
          "phone"
        ],
        "token_endpoint_auth_method": [
          "client_secret_basic",
          "client_secret_post"
        ],
        "backchannel_logout_usable": "true"
      },
      "redirect_uris": [],
      "services": {
        "webfinger": {
          "class": "oidcrp.oidc.webfinger.WebFinger",
          "kwargs": {}
        },
        "discovery": {
          "class": "oidcrp.oidc.provider_info_discovery.ProviderInfoDiscovery",
          "kwargs": {}
        },
        "registration": {
          "class": "oidcrp.oidc.registration.Registration",
          "kwargs": {}
        },
        "authorization": {
          "class": "oidcrp.oidc.authorization.Authorization",
          "kwargs": {}
        },
        "accesstoken": {
          "class": "oidcrp.oidc.access_token.AccessToken",
          "kwargs": {}
        },
        "userinfo": {
          "class": "oidcrp.oidc.userinfo.UserInfo",
          "kwargs": {}
        },
        "end_session": {
          "class": "oidcrp.oidc.end_session.EndSession",
          "kwargs": {}
        }
      }
    }
  }
}
